{
  "editor.accessibilitySupport": "off",
  "editor.find.addExtraSpaceOnTop": false,
  "editor.glyphMargin": true,
  "editor.smoothScrolling": true,
  "editor.suggestSelection": "first",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontLigatures": true,
  "editor.fontFamily": "'Inconsolata'",
  // "editor.fontFamily": "'Fira Code'",
  // "editor.fontFamily": "'Rec Mono Linear'",
  // "editor.fontFamily": "'Jetbrains Mono'",
  // "editor.fontFamily": "'JuliaMono'",
  // "editor.fontFamily": "'IBM Plex Mono'",
  // "editor.fontFamily": "'Cascadia Code'",
  "editor.cursorBlinking": "smooth",
  "editor.fontWeight": "400",
  "editor.suggest.preview": true,
  // "editor.fontSize": 14, // Fira Code
  "editor.fontSize": 17, // for inconsolata
  // "editor.lineHeight": 27,
  "editor.bracketPairColorization.enabled": true,
  // "editor.letterSpacing": -0.1, // For Fira Code
  "editor.lineHeight": 25,
  "editor.minimap.enabled": false,
  // "editor.cursorSmoothCaretAnimation": true,
  "editor.rulers": [
    {
      "column": 80,
      "color": "#4f545a40"
    }
  ],
  "editor.tabSize": 2,
  "editor.cursorStyle": "line",
  // "editor.guides.bracketPairs": "active",
  "editor.cursorWidth": 1,
  "editor.cursorSurroundingLines": 8,
  "editor.insertSpaces": false,
  // "editor.lineNumbers": "relative",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-_",
  "editor.wordWrap": "off",
  "files.trimTrailingWhitespace": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.wrapTabs": false,
  "workbench.editor.decorations.badges": true,
  "breadcrumbs.enabled": false,
  "workbench.colorCustomizations": {
    "[Dark+ Black]": {
      "statusBar.noFolderBackground": "#000",
      "statusBar.debuggingBackground": "#000",
      "editor.background": "#000",
      "activityBar.background": "#000",
      "statusBar.background": "#000",
      "sideBar.background": "#000",
      "statusBar.foreground": "#666666",
      "sideBar.foreground": "#666666",
      "activityBar.activeBorder": "#666666",
      "activityBar.foreground": "#666666",
      "activityBar.inactiveForeground": "#666666",
      "editorLineNumber.foreground": "#303030",
      "editor.lineHighlightBackground": "#181818",
    },
    "[Solarized Dark]": {
      "statusBar.noFolderBackground": "#000",
      "statusBar.debuggingBackground": "#000",
      "editor.background": "#000",
      "activityBar.background": "#000",
      "statusBar.background": "#000",
      "sideBar.background": "#000",
      "statusBar.foreground": "#666666",
      "sideBar.foreground": "#666666",
      "activityBar.activeBorder": "#666666",
      "activityBar.foreground": "#666666",
      "activityBar.inactiveForeground": "#666666",
      "editorLineNumber.foreground": "#303030",
      "editor.lineHighlightBackground": "#131313",
    },
    "[Default Dark+]": {
      "statusBar.background": "#1b1b1b",
      "activityBar.background": "#1b1b1b",
      "sideBar.background": "#1b1b1b",
      "editor.background": "#1b1b1b",
    },
    "[Vitesse Dark]": {
      "editor.background": "#0e0e0e",
    }
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
        ],
        "settings": {
          "fontStyle": "",
        },
      },
    ]
  },
  "workbench.startupEditor": "newUntitledFile",
  "workbench.list.automaticKeyboardNavigation": false,
  "workbench.tree.indent": 20,
  // terminal configs
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.fontWeightBold": "500",
  "terminal.integrated.cursorBlinking": true,
  // "terminal.integrated.fontFamily": "'JetBrainsMono Nerd Font'",
  // "terminal.integrated.fontFamily": "'BlexMono Nerd Font'",
  "terminal.integrated.fontFamily": "'FuraCode Nerd Font'",
  "terminal.integrated.tabs.enabled": true,
  // formatter configs
  "prettier.eslintIntegration": true,
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features",
  },
  "[go]": {
    "editor.formatOnSave": true,
  },
  "[python]": {
    "gitlens.codeLens.symbolScopes": [
      "!Module"
    ],
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnSave": false
  },
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    },
    // "editor.fontFamily": "'SF Mono'",
    // "editor.fontFamily": "'Cascadia Code'",
    // "editor.fontFamily": "'IBM Plex Mono'",
    // "editor.fontFamily": "'Rec Mono Linear'",
    "editor.fontFamily": "'Inconsolata'",
    "editor.fontSize": 17,
    "editor.lineHeight": 24,
    // "editor.fontFamily": "'JuliaMono'",
    // "editor.fontFamily": "'Fira Code'",
    // "editor.fontFamily": "'JuliaMono'",
    // "editor.fontFamily": "'IBM Plex Sans'",
    // "editor.fontFamily": "'Jetbrains Mono'",
    // "editor.letterSpacing": 0.5,
    // "editor.fontFamily": "'Fira Sans'",
    // "editor.fontWeight": "500",
  },
  "[mdx]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    },
    // "editor.fontFamily": "'SF Mono'",
    // "editor.fontFamily": "'IBM Plex Mono'",
    // "editor.fontFamily": "'Fira Code'",
    // "editor.fontFamily": "'JuliaMono'",
    // "editor.fontFamily": "'Jetbrains Mono'",
    "editor.fontFamily": "'Rec Mono Linear'",
    "editor.formatOnSave": false
  },
  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace",
    "gitlens.codeLens.scopes": [
      "document"
    ],
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  // Vim Configs
  "vim.useSystemClipboard": true,
  "vim.hlsearch": true,
  "vim.leader": "<Space>",
  "vim.easymotion": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "J"
      ],
      "after": [
        "5",
        "j"
      ]
    },
    {
      "before": [
        "K"
      ],
      "after": [
        "5",
        "k"
      ]
    },
    {
      "before": [
        "<Leader>",
        "j"
      ],
      "after": [
        "J"
      ]
    },
    {
      "before": [
        "<C-h>"
      ],
      "after": [
        "<C-w>",
        "h"
      ]
    },
    {
      "before": [
        "<C-j>"
      ],
      "after": [
        "<C-w>",
        "j"
      ]
    },
    {
      "before": [
        "<C-k>"
      ],
      "after": [
        "<C-w>",
        "k"
      ]
    },
    {
      "before": [
        "<C-l>"
      ],
      "after": [
        "<C-w>",
        "l"
      ]
    },
    {
      "before": [
        "<tab>"
      ],
      "commands": [
        "workbench.action.nextEditor"
      ]
    },
    {
      "before": [
        "<S-tab>"
      ],
      "commands": [
        "workbench.action.previousEditor"
      ]
    },
    {
      "before": [
        "<Leader>",
        "t",
        "t"
      ],
      "commands": [
        ":tabnew"
      ]
    },
    {
      "before": [
        "<Leader>",
        "t",
        "o"
      ],
      "commands": [
        ":tabo"
      ]
    },
    {
      "before": [
        "<Leader>",
        "t",
        "q"
      ],
      "commands": [
        ":tabclose"
      ]
    },
    {
      "before": [
        "<leader>",
        "e",
        "e"
      ],
      "commands": [
        "workbench.action.toggleSidebarVisibility"
      ]
    },
    {
      "before": [
        "<leader>",
        "e",
        "a"
      ],
      "commands": [
        "workbench.action.toggleActivityBarVisibility"
      ]
    },
    {
      "before": [
        "<leader>",
        "e",
        "q"
      ],
      "commands": [
        "workbench.action.toggleStatusbarVisibility"
      ]
    },
    {
      "before": [
        "<leader>",
        "c",
      ],
      "commands": [
        "editor.action.commentLine"
      ]
    },
    {
      "before": [
        "<leader>",
        "r"
      ],
      "commands": [
        "workbench.action.terminal.toggleTerminal"
      ]
    },
    {
      "before": [
        "<leader>",
        "s"
      ],
      "commands": [
        ":w"
      ]
    }
  ],
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<ESC>"
      ]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [
        "J"
      ],
      "after": [
        "5",
        "j"
      ]
    },
    {
      "before": [
        "<leader>",
        "c",
      ],
      "commands": [
        "editor.action.commentLine"
      ]
    },
    {
      "before": [
        "K"
      ],
      "after": [
        "5",
        "k"
      ]
    },
    {
      "before": [
        "<Leader>",
        "j"
      ],
      "after": [
        "J"
      ]
    },
    {
      "before": [
        "<leader>",
        "t",
        "n",
      ],
      "commands": [
        "editor.lineNumbers",
        "off"
      ]
    },
  ],
  // telementary
  "telemetry.telemetryLevel": "off",
  // misc
  "material-icon-theme.showWelcomeMessage": false,
  "svg.preview.mode": "svg",
  "window.menuBarVisibility": "toggle",
  "workbench.sideBar.location": "right",
  "editor.inlineSuggest.enabled": true,
  // "javascript.inlayHints.parameterNames.enabled": "all",
  // "typescript.inlayHints.parameterNames.enabled": "all",
  "github.copilot.enable": {
    "*": true,
  },
  "cSpell.enableFiletypes": [
    "!handlebars",
    "!haskell",
    "!latex",
    "!php",
    "!pug",
    "!python",
    "!restructuredtext",
    "!rust",
    "!scala"
  ],
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.productIconTheme": "icons-carbon",
  "chaliceIcons.showArrows": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true
  },
  "workbench.editor.untitled.hint": "hidden",
  "gitlens.mode.active": "zen",
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "files.associations": {
    "*.df": "dockerfile"
  },
  "search.exclude": {
    "**/.git": true,
    "**/.github": true,
    "**/.next": true,
    "**/.output": true,
    "**/.pnpm": true,
    "**/.vscode": true,
    "**/.yarn": true,
    "**/dist/**": true,
    "**/logs": true,
    "**/node_modules": true,
    "**/out/**": true,
    "**/package-lock.json": true,
    "**/tmp": true,
    "**/yarn.lock": true
  },
  "workbench.iconTheme": "file-icons",
  "workbench.localHistory.enabled": true,
  "css.format.enable": true,
  "workbench.colorTheme": "Themer Dark",
  "redhat.telemetry.enabled": false,
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "[svg]": {
    "editor.defaultFormatter": "jock.svg"
  },
  "workbench.statusBar.visible": false,
  "[elixir]": {
    "editor.defaultFormatter": "JakeBecker.elixir-ls"
  },
  "window.zoomLevel": -0.2
}
